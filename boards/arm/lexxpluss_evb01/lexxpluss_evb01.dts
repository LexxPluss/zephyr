/dts-v1/;
#include <st/f7/stm32f765Xi.dtsi>
#include <st/f7/stm32f765b(g-i)tx-pinctrl.dtsi>

/ {
	model = "LexxPluss Main Control Unit evaluation board";
	compatible = "st,stm32f765bi-lexxpluss";

	chosen {
		zephyr,console = &usart3;
		zephyr,shell-uart = &usart3;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,dtcm = &dtcm;
		zephyr,can-primary = &can1;
	};

	leds {
		compatible = "gpio-leds";
		user_led_0: led_0 {
			gpios = <&gpioj 0 GPIO_ACTIVE_HIGH>;
			label = "User LED0";
		};
		user_led_1: led_1 {
			gpios = <&gpioj 1 GPIO_ACTIVE_HIGH>;
			label = "User LED1";
		};
		user_led_2: led_2 {
			gpios = <&gpioj 2 GPIO_ACTIVE_HIGH>;
			label = "User LED2";
		};
		user_led_3: led_3 {
			gpios = <&gpioj 3 GPIO_ACTIVE_HIGH>;
			label = "User LED3";
		};
	};

	sensors {
		us0: hc-sr04-0 {
			compatible = "elecfreaks,hc-sr04";
			label = "HC-SR04_0";
			trig-gpios = <&gpiog 2 GPIO_ACTIVE_HIGH>;
			echo-gpios = <&gpiog 3 GPIO_ACTIVE_HIGH>;
			status = "okay";
		};
		us1: hc-sr04-1 {
			compatible = "elecfreaks,hc-sr04";
			label = "HC-SR04_1";
			trig-gpios = <&gpiok 1 GPIO_ACTIVE_HIGH>;
			echo-gpios = <&gpiok 2 GPIO_ACTIVE_HIGH>;
			status = "okay";
		};
		us2: hc-sr04-2 {
			compatible = "elecfreaks,hc-sr04";
			label = "HC-SR04_2";
			trig-gpios = <&gpiog 4 GPIO_ACTIVE_HIGH>;
			echo-gpios = <&gpiog 5 GPIO_ACTIVE_HIGH>;
			status = "okay";
		};
		us3: hc-sr04-3 {
			compatible = "elecfreaks,hc-sr04";
			label = "HC-SR04_3";
			trig-gpios = <&gpiog 6 GPIO_ACTIVE_HIGH>;
			echo-gpios = <&gpiog 7 GPIO_ACTIVE_HIGH>;
			status = "okay";
		};
	};

	aliases {
		led0 = &user_led_0;
		led1 = &user_led_1;
		led2 = &user_led_2;
		led3 = &user_led_3;
	};
};

&usart3 {
	pinctrl-0 = <&usart3_tx_pd8 &usart3_rx_pd9>;
	current-speed = <115200>;
	status = "okay";
};

&uart7 {
	pinctrl-0 = <&uart7_rx_pe7 &uart7_tx_pe8 &uart7_rts_pf8 &uart7_cts_pf9>;
	current-speed = <115200>;
	status = "okay";
};

&usbotg_fs {
	pinctrl-0 = <&usb_otg_fs_dm_pa11 &usb_otg_fs_dp_pa12>;
	status = "okay";
};

&can1 {
	pinctrl-0 = <&can1_rx_ph14 &can1_tx_ph13>;
	bus-speed = <125000>;
	sjw = <1>;
	prop-seg = <0>;
	phase-seg1 = <6>;
	phase-seg2 = <5>;
	status = "okay";
};

&spi1 {
	pinctrl-0 = <&spi1_nss_pg10 &spi1_sck_pg11 &spi1_miso_pg9 &spi1_mosi_pd7>;
	status = "okay";
	adis16470@0 {
		compatible = "adi,adis16470";
		reg = <0>;
		spi-max-frequency = <1000000>;
		label= "ADIS16470";
	};
};

&spi4 {
	pinctrl-0 = <&spi4_nss_pe4 &spi4_sck_pe2 &spi4_miso_pe13 &spi4_mosi_pe6>;
	status = "okay";
	led_strip0: ws2812@0 {
		compatible = "worldsemi,ws2812-spi";
		label = "WS2812_0";
		reg = <0>;
		spi-max-frequency = <8000000>;
		chain-length = <8>;
		spi-one-frame = <0x7c>;
		spi-zero-frame = <0x60>;
	};

};

&spi5 {
	pinctrl-0 = <&spi5_nss_ph5 &spi5_sck_pf7 &spi5_miso_ph7 &spi5_mosi_pf11>;
	status = "okay";
	led_strip1: ws2812@1 {
		compatible = "worldsemi,ws2812-spi";
		label = "WS2812_1";
		reg = <0>;
		spi-max-frequency = <8000000>;
		chain-length = <8>;
		spi-one-frame = <0x7c>;
		spi-zero-frame = <0x60>;
	};

};

&timers10 {
	status = "okay";
	pwm {
		pinctrl-0 = <&tim10_ch1_pb8>;
		status = "okay";
	};
};

&timers11 {
	status = "okay";
	pwm {
		pinctrl-0 = <&tim11_ch1_pb9>;
		status = "okay";
	};
};

&sdmmc1 {
	pinctrl-0 = <&sdmmc1_d0_pc8 &sdmmc1_d1_pc9 &sdmmc1_d2_pc10 &sdmmc1_d3_pc11 &sdmmc1_ck_pc12 &sdmmc1_cmd_pd2>;
	status = "okay";
};
